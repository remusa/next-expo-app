{
  "name": "solito-blank",
  "private": true,
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.7",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@ls-lint/ls-lint": "^1.11.0",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.39",
    "@types/react-native": "^0.67.2",
    "eslint": "^8.11.0",
    "is-ci-cli": "^2.2.0",
    "jsdom": "^19.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.1",
    "turbo": "^1.1.6",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vitest": "^0.7.11"
  },
  "scripts": {
    "native": "cd apps/expo && expo start",
    "web": "cd apps/next && pnpm next",
    "dev": "run-p native web",
    "build": "",
    "build:ci": "",
    "test": "is-ci-cli \"test:coverage\" \"test:watch\"",
    "test:watch": "vitest --passWithNoTests",
    "test:ui": "vitest ui",
    "test:coverage": "vitest run --coverage",
    "test:test": "vitest run",
    "test:ci": "cross-env NODE_ENV=production vitest related --passWithNoTests",
    "setup": "pnpm --silent && pnpm build",
    "clean": "run-p clean:*",
    "clean:build": "rm -rf build/ dist/",
    "clean:deps": "rm -rf node_modules",
    "deps:check": "pnpx npm-check -u",
    "deps:upgrade:all": "pnpx ncu -u --doctor",
    "typecheck": "tsc -noEmit --preserveWatchOutput",
    "typecheck:watch": "pnpm typecheck -- -w",
    "lslint": "node_modules/.bin/ls-lint",
    "prettier": "prettier --ignore-path .gitignore --loglevel=silent \"**/*.+(js|jsx|ts|tsx|json|css|html|md|mdx)\"",
    "eslint": "eslint --ignore-path .gitignore --quiet .",
    "lint": "run-p -r lslint typecheck prettier eslint",
    "fix:prettier": "pnpm prettier -- --write",
    "fix:eslint": "pnpm eslint -- --cache --fix",
    "fix": "run-p -r fix:prettier fix:eslint",
    "commitlint": "pnpx --no -- commitlint --edit '$1'",
    "version:init": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 8.0.0"
  }
}
